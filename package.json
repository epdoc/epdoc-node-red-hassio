{
  "name": "epdoc-node-red-hassio",
  "version": "0.16.0",
  "description": "Node-RED code for my own Home Assistant deployment",
  "private": false,
  "modules": "src/index.ts",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "James Pravetz",
    "email": "jpravetz@epdoc.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpravetz/epdoc-node-red-hassio.git"
  },
  "scripts": {
    "build_failed": "bun build ./src/index.ts --target node --outdir ./dist && tsc",
    "build": "tsc",
    "clean": "rm -rf dist docs && echo 'Done.'",
    "coverage": "bun test --coverage",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts' '*.js' '*.json'",
    "docs": "./node_modules/typedoc/bin/typedoc"
  },
  "devDependencies": {
    "bun-types": "latest",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^49.0.0",
    "prettier": "^3.0.3",
    "typedoc": "^0.25.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "epdoc-node-red-hautil": "latest",
    "epdoc-timeutil": "latest",
    "epdoc-util": "latest"
  }
}
