{
  "name": "epdoc-node-red-hassio",
  "version": "1.0.0",
  "description": "Node-RED code for my own Home Assistant deployment",
  "private": false,
  "main": "dist/index.js",
  "keywords": [
    "xnode-red"
  ],
  "node-red": {
    "version": ">=3.0.0",
    "nodes": {
      "fan-control": "./dist/nodes/fan-control/fan-control-node.js",
      "lower-case": "./dist/nodes/lower-case/lower-case.js"
    }
  },
  "license": "MIT",
  "author": {
    "name": "James Pravetz",
    "email": "jpravetz@epdoc.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpravetz/epdoc-node-red-hassio.git"
  },
  "scripts": {
    "clean": "rm -rf dist docs coverage && echo Done.",
    "build": "rm -rf dist && tsc && npm run copy",
    "copy": "copyfiles -u 1 './src/**/*.{css,html}' './dist/'",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts' '*.js' '*.json'",
    "docs": "./node_modules/typedoc/bin/typedoc",
    "test": "jest -t lower-case-node",
    "patch": "rm -rf node_modules/epdoc-node-red-hautil && cp -R /Users/jpravetz/dev/epdoc/hass/epdoc-node-red-hautil ./node_modules && echo Done."
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.10",
    "@types/node-red": "^1.3.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^49.0.0",
    "jest": "^29.7.0",
    "node-red": "^3.1.0",
    "node-red-node-test-helper": "^0.3.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "should": "^13.2.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "epdoc-node-red-hautil": "^1.0.3",
    "epdoc-timeutil": "^2.3.6",
    "epdoc-util": "^0.5.1"
  }
}
